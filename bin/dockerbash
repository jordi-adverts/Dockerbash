#!/usr/bin/env ruby

begin
  require 'dockerbash'
rescue LoadError
  require 'rubygems'
  require 'dockerbash'
end

# Clean exit with CTRL+C
trap("INT") { exit }

dockerbash = Dockerbash::Client.new("http://localhost", "4243")
dockerbash.select_docker_container(ids=[], tmp_work = '/tmp/docker-bash/')
